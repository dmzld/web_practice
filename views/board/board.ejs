<!doctype html>
<html>
    <head>
        <title>이윤석의 web</title>
        <meta charset="utf-8">
        <meta name="keyword" content="이윤석,코딩,html,web,웹">
        <meta name="description" content="html 연습한 page"> 
        <meta http-equiv="refresh" content="30">
    
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <link rel="stylesheet" href="/stylesheet/stylesheet.css">
    </head>
    
    <body>
        <div class="container">
            <nav>
                <% include ../partials/nav %>
            </nav>

            <header>
                <h2>Board</h2>    
            </header>
            
            <main>
                <section>
                    <ul>
                        <% boards.forEach(function(boards) { %>
                            <li>
                              <a href="/board/<%= boards._id %>"><span><%= boards.title %></span><span style="float: right"><%= boards.date %></span></a>
                            </li>
                          <% }) %>            
                    </ul>
                </section>
                <div>
                    <a href="/board/new" style="float:right">Write</a>
                </div>
            </main>
            <footer>
                <a href="https://github.com/dmzld">GitHub - dmzld</a>
            </footer>
        </div>
        </div>
    </body>
</html>